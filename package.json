{
  "name": "rexpack",
  "version": "1.0.0",
  "description": "Expack is the bare-bones Express and Webpack boilerplate with ES6+ babel transpilation, ESLint linting, Hot Module Reloading, and Jest test framework enabled.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "start": "node ./dist/server.js"
  },
  "repository": "https://github.com/bengrunfeld/rexpack",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "axios": "^0.20.0",
    "babel-plugin-stylus-compiler": "^1.4.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-pug": "^7.0.1",
    "connect-redis": "^5.0.0",
    "eslint-plugin-react-pug": "^0.8.4",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "lodash": "^4.17.20",
    "mobx": "^5.15.7",
    "mobx-react-lite": "^2.2.2",
    "mongodb": "^3.6.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-vkontakte": "^0.5.0",
    "pokedex-promise-v2": "^3.2.0",
    "pokemon": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.7.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "redis": "^3.0.2",
    "stylus": "^0.54.7"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
